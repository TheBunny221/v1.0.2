{
  "name": "cochin-smart-city",
  "private": true,
  "version": "1.0.0",
  "description": "Cochin Smart City Complaint Management System",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "vite",
    "server:dev": "nodemon server/server.js",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "start-server-and-test 'npm run dev' http://localhost:3000 'npm run cypress:run'",
    "seed": "node prisma/seed.js",
    "seed:prod": "NODE_ENV=production node prisma/seed-production.js",
    "seed:dev": "NODE_ENV=development node prisma/seed.js",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:create": "npx prisma migrate dev --create-only",
    "db:migrate:deploy": "npx prisma migrate deploy",
    "db:migrate:status": "npx prisma migrate status",
    "db:migrate:reset": "npx prisma migrate reset --force",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:pull": "npx prisma db pull",
    "db:studio": "npx prisma studio",
    "db:validate": "npx prisma validate",
    "db:format": "npx prisma format",
    "db:reset": "npx prisma migrate reset --force",
    "db:setup": "npm run db:generate && npm run db:migrate:deploy && npm run seed",
    "db:setup:prod": "npm run db:generate && npm run db:migrate:deploy && npm run seed:prod",
    "db:setup:fresh": "npm run db:migrate:reset && npm run db:setup",
    "db:backup": "node prisma/migration-utils.js backup",
    "db:restore": "node prisma/migration-utils.js restore",
    "db:stats": "node prisma/migration-utils.js stats",
    "db:cleanup": "node prisma/migration-utils.js cleanup",
    "db:check": "node prisma/migration-utils.js check",
    "postinstall": "npm run db:generate"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/multer": "^1.4.11",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
