{
  "name": "cochin-smart-city",
  "private": true,
  "version": "1.0.0",
  "description": "Cochin Smart City Complaint Management System",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "dev:start": "npm run dev:setup && npm run dev",
    "dev:setup": "node scripts/setup-dev-environment.js setup",
    "dev:reset": "node scripts/setup-dev-environment.js reset",
    "client:dev": "vite",
    "server:dev": "nodemon server/server.js",
    "server:prod": "node server/server.js",
    "build": "tsc && vite build",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "start-server-and-test 'npm run dev' http://localhost:3000 'npm run cypress:run'",
    "seed:dev": "NODE_ENV=development node prisma/seed.dev.js",
    "seed:prod": "node prisma/seed.prod.js",
    "seed": "node prisma/seed.js",
    "db:generate:dev": "npx prisma generate --schema=prisma/schema.dev.prisma",
    "db:generate:prod": "npx prisma generate --schema=prisma/schema.prod.prisma",
    "db:generate": "npx prisma generate",
    "db:migrate:dev": "npx prisma migrate dev --schema=prisma/schema.dev.prisma",
    "db:migrate:prod": "npx prisma migrate deploy --schema=prisma/schema.prod.prisma",
    "db:migrate:create": "npx prisma migrate dev --create-only --schema=prisma/schema.dev.prisma",
    "db:migrate:status": "npx prisma migrate status",
    "db:migrate:reset:dev": "npx prisma migrate reset --force --schema=prisma/schema.dev.prisma",
    "db:migrate:reset:prod": "npx prisma migrate reset --force --schema=prisma/schema.prod.prisma",
    "db:migrate:reset": "npx prisma migrate reset --force",
    "db:push:dev": "npx prisma db push --schema=prisma/schema.dev.prisma",
    "db:push:prod": "npx prisma db push --schema=prisma/schema.prod.prisma",
    "db:push": "npx prisma db push",
    "db:studio:dev": "npx prisma studio --schema=prisma/schema.dev.prisma",
    "db:studio:prod": "npx prisma studio --schema=prisma/schema.prod.prisma",
    "db:studio": "npx prisma studio",
    "db:validate": "npx prisma validate",
    "db:format": "npx prisma format",
    "db:setup:dev": "npm run db:generate:dev && npm run db:push:dev && npm run seed:dev",
    "db:setup:prod": "npm run db:generate:prod && npm run db:migrate:prod && npm run seed:prod",
    "db:setup": "npm run db:generate && npm run db:migrate:prod && npm run seed",
    "db:setup:fresh:dev": "npm run db:migrate:reset:dev && npm run db:setup:dev",
    "db:setup:fresh:prod": "npm run db:migrate:reset:prod && npm run db:setup:prod",
    "db:setup:fresh": "npm run db:migrate:reset && npm run db:setup",
    "db:backup": "node prisma/migration-utils.js backup",
    "db:restore": "node prisma/migration-utils.js restore",
    "db:stats": "node prisma/migration-utils.js stats",
    "db:cleanup": "node prisma/migration-utils.js cleanup",
    "db:check": "node prisma/migration-utils.js check",
    "postinstall": "npm run db:generate:dev",
    "validate:db": "node scripts/validate-db-env.js",
    "preserver:prod": "node scripts/validate-db-env.js && npx prisma migrate deploy --schema=prisma/schema.prod.prisma && npx prisma generate --schema=prisma/schema.prod.prisma",
    "db:dev": "npx prisma migrate dev --schema=prisma/schema.dev.prisma --name init && npx prisma generate --schema=prisma/schema.dev.prisma",
    "db:prod": "npx prisma migrate deploy --schema=prisma/schema.prod.prisma && npx prisma generate --schema=prisma/schema.prod.prisma",
    "db:reset": "npx prisma migrate reset --force",
    "setup:dev": "npm run db:dev && npm run dev",
    "setup:prod": "npm run preserver:prod && npm run server:prod",
    "preserver:dev": "node scripts/validate-db-env.js && npx prisma generate --schema=prisma/schema.dev.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/leaflet": "^1.9.20",
    "@types/multer": "^1.4.11",
    "@types/react-window": "^1.8.8",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "html2canvas": "^1.4.1",
    "i18next": "^25.5.2",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "lucide-react": "^0.303.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-i18next": "^15.7.3",
    "react-leaflet": "^5.0.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.20.1",
    "react-window": "^1.8.11",
    "recharts": "^2.8.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/leaflet-draw": "^1.0.12",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "autoprefixer": "^10.4.16",
    "compression": "^1.8.1",
    "concurrently": "^8.2.2",
    "cypress": "^13.6.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "express-validator": "^7.2.1",
    "jsdom": "^23.0.1",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "prisma": "^5.7.1",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "prisma": {
    "seed": "node prisma/seed.prod.js"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
